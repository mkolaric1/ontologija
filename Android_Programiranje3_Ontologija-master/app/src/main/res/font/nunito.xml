<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- Nunito Extra Light-->
    <font  android:fontStyle="normal" android:fontWeight="200" android:font="@font/nunitoextralight"/>

    <!-- Nunito Extra Light Italic-->
    <font android:fontStyle="italic" android:fontWeight="200" android:font="@font/nunitoextralightitalic"/>

    <!-- Nunito Light -->
    <font android:fontStyle="normal" android:fontWeight="300" android:font="@font/nunitolight"/>

